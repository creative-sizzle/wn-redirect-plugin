{
    "name": "creative-sizzle/wn-redirect-plugin",
    "description": "Advanced redirect plugin for Winter CMS.",
    "license": "GPL-2.0-only",
    "type": "winter-plugin",
    "authors": [
        {
            "name": "Van der Let & Partners",
            "email": "octobercms@vdlp.nl",
            "role": "Original Author"
        },
        {
            "name": "Alwin Drenth",
            "email": "adrenth@gmail.com",
            "role": "Original Author"
        },
        {
            "name": "Eric Pfeiffer",
            "email": "eric@creativesizzle.com",
            "role": "Lead Maintainer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "composer/installers": "~2.0",
        "davaxi/sparkline": "^2.1",
        "jaybizzle/crawler-detect": "^1.2",
        "winter/storm": "^1.1"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23",
        "friendsofphp/php-cs-fixer": "^3.4",
        "symfony/stopwatch": "^5.4 || ^6.0"
    },
    "replace": {
        "vdlp/oc-redirect-plugin": "<4.0.0"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "archive": {
        "exclude": [
            ".gitattributes",
            ".github",
            ".gitignore",
            "tests",
            "phpunit.xml"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    },
    "extra": {
        "installer-name": "redirect"
    },
    "scripts": {
        "format": "vendor/bin/php-cs-fixer fix --allow-risky=yes"
    }
}
