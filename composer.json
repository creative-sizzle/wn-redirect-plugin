{
    "name": "creative-sizzle/wn-redirect-plugin",
    "description": "Advanced redirect plugin for Winter CMS.",
    "license": "GPL-2.0-only",
    "type": "winter-plugin",
    "authors": [
        {
            "name": "Van der Let & Partners",
            "email": "octobercms@vdlp.nl",
            "role": "Original Author"
        },
        {
            "name": "Alwin Drenth",
            "email": "adrenth@gmail.com",
            "role": "Original Author"
        },
        {
            "name": "Eric Pfeiffer",
            "email": "eric@creativesizzle.com",
            "role": "Lead Maintainer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "ext-curl": "*",
        "ext-json": "*",
        "composer/installers": "~1.0",
        "davaxi/sparkline": "^2.1",
        "jaybizzle/crawler-detect": "^1.2"
    },
    "require-dev": {
        "ergebnis/composer-normalize": "^2.23",
        "symfony/stopwatch": "^5.3 || ^6.0"
    },
    "archive": {
        "exclude": [
            ".gitattributes",
            ".github",
            ".gitignore",
            "tests",
            "phpunit.xml"
        ]
    },
    "config": {
        "allow-plugins": {
            "composer/installers": true,
            "ergebnis/composer-normalize": true
        },
        "sort-packages": true
    }
}
